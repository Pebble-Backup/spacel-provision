{
  "name": "http-env-echo",
  "health_check": "HTTP:80/",
  "elastic_ips": "enabled",
  "instance_availability": "internet-facing",
  "instance_type": "t2.nano",
  "instance_min": 1,
  "instance_max": 1,
  "services": {
    "http-env-echo": {
      "image": "pwagner/http-env-echo",
      "ports": {
        "80": 8080
      }
    }
  },
  "public_ports": {
    "80": {
      "sources": ["0.0.0.0/0"]
    },
    "443": {
      "sources": ["0.0.0.0/0"],
      "internal_port": 80
    }
  }
}
